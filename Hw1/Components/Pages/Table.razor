@page "/Table"

<h3>Table</h3>


<table class="table table-bordered" style="width: 50%; margin: 0 auto;">
    <tr>
        <th>X</th>
        @for(int i = 1; i <= 10; i++)
        {
            <th>@i</th>
        }
    </tr>
    @for (int k = 1; k <= 10; k++)
    {
        <tr>
            <td class="Q1"> @k</td>
            @for (int j = 1; j <= 10; j++)
            {
                <td class="Mo1">@(k * j)</td>
            }

        </tr>
    }

</table>
@code {

}
